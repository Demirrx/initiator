{
    "name": "benlumia007/initiator",
    "description": "Initiator is a starter theme designed to help theme authors using Backdrop Core Framework.",
    "keywords": [ "classicpress", "wordpress", "theme" ],
    "homepage": "https://github.com/benlumia007/initiator",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benjamin Lu",
            "email": "benlumia007@gmail.com",
            "homepage": "https://benjlu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Initiator\\": "app/"
		},
		"files":[
            "app/Entry/functions-entry.php",
            "app/Site/functions-site.php",
			"app/framework.php",
			"app/functions-filters.php",
			"app/functions-scripts.php",
			"app/functions-setup.php"
		]
    },
    "require": {
        "php": ">=7.1",
        "backdrop-dev/framework": "^1.0",
		"backdrop-dev/customize": "^1.0",
		"backdrop-dev/fonts": "^1.0",
		"backdrop-dev/mix": "^1.0",
		"backdrop-dev/template-hierarchy": "^1.0",
		"backdrop-dev/template-manager": "^1.0",
		"backdrop-dev/view": "^1.0",
		"backdrop-dev/theme": "^1.0"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}